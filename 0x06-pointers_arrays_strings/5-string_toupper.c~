#include "main.h"

/**
 * string_toupper -  changes all lowercase letters of a string to uppercase.
 *
 * @char: string to be converted
 *
 * Return:void
 */

char *string_toupper(char *)
{
	int i;

	i = 0;
	while (*(s + i))
	{
		if (*(s + i) >= 'a' && *(s + i) <= 'z')
			*(s + i) -= 'a' - 'A';
		i++;
	}
	return (s);
}
